<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.hnust.icanjiuye">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".MainApplication"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity" android:launchMode="singleInstance" android:theme="@style/AppTheme.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".MainActivityBusiness"/>
        <activity android:name=".RegisterActivity"/>
        <activity android:name=".CommunityActivity"/>
        <activity android:name=".HelpActivity"/>
        <activity android:name=".JobActivity"/>
        <activity android:name=".PersonCenterActivity"/>
        <activity android:name=".StartJobActivity"/>
        <activity android:name=".TrainActivity"/>
        <activity android:name=".SearchActivity"/>
        <activity android:name=".DetailActivity"/>
        <activity android:name=".JobDetailActivity"/>
        <service android:name=".service.StartJobService"/>
    </application>

</manifest>
